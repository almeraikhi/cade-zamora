{
  "name": "cade-zamora",
  "version": "0.2.0",
  "private": true,
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "yarn db:migrate && node server.js",
    "lint": "next lint",
    "dx": "yarn s3:clean && yarn uploads:clean && yarn db:generate && yarn db:push && yarn db:seed",
    "s3:up": "docker compose -f ./integrations/minio/docker-compose.yaml up -d",
    "s3:clean": "tsx ./scripts/cleanMinio.ts",
    "db:up": "docker compose -f ./integrations/postgres/docker-compose.yaml up -d",
    "db:seed": "prisma db seed",
    "db:generate": "prisma generate",
    "db:push": "prisma db push --skip-generate --accept-data-loss --force-reset",
    "db:migrate": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "uploads:clean": "rm -rf ./public/uploads/*"
  },
  "dependencies": {
